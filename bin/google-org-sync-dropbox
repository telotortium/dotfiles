#!/usr/bin/env bash
# Merge changes in google-org from Dropbox into repo, and then upload repo
# contents back to Dropbox for use by Orgzly.
#
# Options:
#   -l: Use rsync and a local directory instead of dbxcli

set -eu
set -o pipefail

org_repo=$HOME/Documents/org/google-org
dropbox_local_root=$HOME/Dropbox
dropbox_org=/google-org

source "$(dirname ${BASH_SOURCE[0]})/sync-dropbox.bash"
